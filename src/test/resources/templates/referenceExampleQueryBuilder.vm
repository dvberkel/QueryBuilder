package org.effrafax.querybuilder.generated.builder;

import org.effrafax.querybuilder.core.QueryBuilder;
import org.effrafax.querybuilder.core.criteria.LongPropertyCriterium;
import org.effrafax.querybuilder.core.criteria.PropertyCriterium;
import org.effrafax.querybuilder.core.criteria.StringPropertyCriterium;
import org.effrafax.querybuilder.test.Example;

public class ExampleQueryBuilder extends QueryBuilder<Example>
{

	public ExampleQueryBuilder()
	{
		super(Example.class);
	}

	public PropertyCriterium<Example, String> name()
	{
		PropertyCriterium<Example, String> propertyCriterium = new StringPropertyCriterium<Example>("name");
		registerPropertyCriterium(propertyCriterium);
		return propertyCriterium;
	}

	public PropertyCriterium<Example, Long> id()
	{
		PropertyCriterium<Example, Long> propertyCriterium = new LongPropertyCriterium<Example>("id");
		registerPropertyCriterium(propertyCriterium);
		return propertyCriterium;
	}

}
